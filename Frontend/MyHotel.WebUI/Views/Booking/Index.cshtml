
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("Head")

<body>
    <div class="container-fluid bg-white p-0">

        @await Component.InvokeAsync("Spinner")

        @await Component.InvokeAsync("Navbar")

        @await Component.InvokeAsync("BookingHeader")

        @await Component.InvokeAsync("Reservation")

        @await Html.PartialAsync("/Views/Booking/AddBooking.cshtml")

        @await Html.PartialAsync("/Views/Default/SubscribePartial.cshtml")

        @await Component.InvokeAsync("Footer")

        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>

    @await Component.InvokeAsync("Script")
</body>

</html>