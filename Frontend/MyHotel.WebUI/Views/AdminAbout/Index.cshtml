@model List<ListAboutDto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>#</th>
            <th>Başlık 1</th>
            <th>Başlık 2</th>
            <th>Güncelle</th>
        </tr>
    </thead>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutId</td>
            <td>@item.Title1</td>
            <td>@item.Title2</td>

            <td><a href="/AdminAbout/UpdateAbout/@item.AboutId" class="btn btn-dark"> Güncelle </a></td>
        </tr>
    }
</table>



